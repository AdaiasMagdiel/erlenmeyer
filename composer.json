{
    "name": "adaiasmagdiel/erlenmeyer",
    "description": "Erlenmeyer is a lightweight PHP framework designed to create web applications simply and efficiently.",
    "type": "library",
    "license": "GPL-3.0-only",
    "autoload": {
        "psr-4": {
            "AdaiasMagdiel\\Erlenmeyer\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Ada√≠as Magdiel",
            "email": "adaiasmagdiell@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require-dev": {
        "pestphp/pest": "^3.8"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
